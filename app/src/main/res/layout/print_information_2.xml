<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:id="@+id/name_label5"
        android:layout_marginTop="33dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/positionTextView5"
        android:layout_alignStart="@+id/positionTextView5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Company Name"
        android:id="@+id/companyName_label5"
        android:layout_below="@+id/positionTextView5"
        android:layout_alignLeft="@+id/phon_label5"
        android:layout_alignStart="@+id/phon_label5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone Number"
        android:id="@+id/phon_label5"
        android:layout_below="@+id/comNameTextView5"
        android:layout_alignLeft="@+id/fax_label5"
        android:layout_alignStart="@+id/fax_label5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fax Number"
        android:id="@+id/fax_label5"
        android:layout_below="@+id/phoneTextView5"
        android:layout_alignLeft="@+id/email_label5"
        android:layout_alignStart="@+id/email_label5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E-mail"
        android:id="@+id/email_label5"
        android:layout_below="@+id/faxTextView5"
        android:layout_marginLeft="31dp"
        android:layout_marginStart="31dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/nameTextView5"
        android:layout_below="@+id/name_label5"
        android:layout_alignLeft="@+id/name_label5"
        android:layout_alignStart="@+id/name_label5" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/comNameTextView5"
        android:layout_below="@+id/companyName_label5"
        android:layout_alignLeft="@+id/phon_label5"
        android:layout_alignStart="@+id/phon_label5" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/phoneTextView5"
        android:layout_below="@+id/phon_label5"
        android:layout_alignLeft="@+id/phon_label5"
        android:layout_alignStart="@+id/phon_label5" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/faxTextView5"
        android:layout_below="@+id/fax_label5"
        android:layout_alignLeft="@+id/email_label5"
        android:layout_alignStart="@+id/email_label5" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/emailTextView5"
        android:layout_below="@+id/email_label5"
        android:layout_alignLeft="@+id/email_label5"
        android:layout_alignStart="@+id/email_label5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edit"
        android:id="@+id/informEditBtn5"
        android:layout_marginBottom="68dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/fax_label5"
        android:layout_toEndOf="@+id/fax_label5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SAVE"
        android:id="@+id/saveBtn5"
        android:layout_alignTop="@+id/informEditBtn5"
        android:layout_toRightOf="@+id/informEditBtn5"
        android:layout_toEndOf="@+id/informEditBtn5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Position"
        android:id="@+id/position_label5"
        android:layout_below="@+id/nameTextView5"
        android:layout_alignLeft="@+id/companyName_label5"
        android:layout_alignStart="@+id/companyName_label5" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/positionTextView5"
        android:layout_below="@+id/position_label5"
        android:layout_alignLeft="@+id/companyName_label5"
        android:layout_alignStart="@+id/companyName_label5" />
</RelativeLayout>